upstream django {
  server unix:///home/ubuntu/spectacles/uwsgi.sock;
}

server {
  listen 80;
  server_name spectacles.pw;
  charset utf-8;
  location /static {
    alias /home/ubuntu/spectacles/static;
  }
  location / {
    uwsgi_pass django;
    include /home/ubuntu/spectacles/config/uwsgi_params;
  }
}
